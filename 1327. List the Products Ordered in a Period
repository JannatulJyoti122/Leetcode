# Write your MySQL query statement below
select product_name,unit
from(
select *
from products p join(
select product_id as new_id, sum(unit) as unit
from orders
where order_date>='2020-02-01' and order_date<'2020-03-01'
group by product_id order by product_id) sum_order
where sum_order.new_id=p.product_id) total
where total.unit>=100
